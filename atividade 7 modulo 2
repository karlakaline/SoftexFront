eleitores = int(input("Digite o n√∫mero total de eleitores: "))
candidatoa = 0
candidatob = 0
candidatoc = 0
votantes = 0
while (votantes < eleitores):
voto = int(input("Digite 889 para votar no candidato_X,
 847 para o candidato_y e 515 para o candidato_Z"))
if (voto == 889):
candidatoa = candidatoa + 1
elif (voto == 847):
candidatob = candidatob + 1
elif (voto == 515):
candidatoc = candidatoc + 1
votantes = votantes + 1
print("O candidato A teve", candidato_X, "votos.")
print("O candidato B teve", candidato_Y, "votos.")
print("O candidato C teve", candidato_Z, "votos.")
